@using EntityFramworkExercise.Models
@model IEnumerable<Todo>

<html>
    <head>
        <title>List</title>
    </head>

    <body>
        <div>
            <h1>Todos for today:</h1>
        </div>
        <ul>
            @foreach (var task in @Model)
            {
                <li>@task.Title</li>
            }
        </ul>
    </body>
</html>